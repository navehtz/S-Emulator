<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>

<?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.components.InstructionTableController" fillWidth="true">
  <TableView fx:id="table" VBox.vgrow="ALWAYS" prefHeight="307.0" prefWidth="652.0" >
      <columnResizePolicy>
          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
      </columnResizePolicy>

      <columns>
          <TableColumn fx:id="colIdx" prefWidth="60" style="-fx-alignment: CENTER;" text="#" />
          <TableColumn fx:id="colType" prefWidth="70" style="-fx-alignment: CENTER;" text="B/S" />
          <TableColumn fx:id="colLabel" prefWidth="110" style="-fx-alignment: CENTER;" text="Label" />
          <TableColumn fx:id="colCommand" prefWidth="320" style="-fx-alignment: CENTER;" text="Instruction" />
          <TableColumn fx:id="colCycles" prefWidth="90" style="-fx-alignment: CENTER;" text="Cycles" />
      </columns>
  </TableView>
</VBox>
