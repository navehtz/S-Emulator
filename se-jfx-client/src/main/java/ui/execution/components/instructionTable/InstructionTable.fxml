<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fillWidth="true" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.execution.components.instructionTable.InstructionTableController">
  <TableView fx:id="table" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">

      <columns>
          <TableColumn fx:id="colBp" maxWidth="-1.0" minWidth="25.0" prefWidth="-1.0" text="" />
          <TableColumn fx:id="colIdx" maxWidth="-1.0" minWidth="50.0" prefWidth="-1.0" style="-fx-alignment: CENTER;" text="#" />
          <TableColumn fx:id="colType" maxWidth="-1.0" minWidth="50.0" prefWidth="-1.0" style="-fx-alignment: CENTER;" text="B/S" />
          <TableColumn fx:id="colLabel" maxWidth="-1.0" minWidth="80.0" prefWidth="-1.0" style="-fx-alignment: CENTER;" text="Label" />
          <TableColumn fx:id="colCommand" maxWidth="1.7976931348623157E308" minWidth="200.0" prefWidth="-1.0" style="-fx-alignment: CENTER;" text="Instruction" />
          <TableColumn fx:id="colCycles" maxWidth="-1.0" minWidth="80.0" prefWidth="-1.0" style="-fx-alignment: CENTER;" text="Cycles" />
          <TableColumn fx:id="colArchitecture" maxWidth="-1.0" minWidth="80.0" prefWidth="-1.0" text="Architecture" />
      </columns>
      <columnResizePolicy>
          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
      </columnResizePolicy>
  </TableView>
</VBox>
