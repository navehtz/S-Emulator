<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<TitledPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="User History" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.dashboard.components.userHistoryTable.UserHistoryTableController">
      <content>
          <VBox alignment="TOP_RIGHT" prefHeight="200.0" prefWidth="597.0">
              <children>
                  <TableView maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="199.0" prefWidth="467.0" VBox.vgrow="ALWAYS">
                      <columns>
                          <TableColumn fx:id="colRunNum" prefWidth="55.0" text="#" />
                          <TableColumn fx:id="colMainProgramOrFunction" prefWidth="105.0" text="Program/Function" />
                          <TableColumn fx:id="colProgramName" prefWidth="75.0" text="Program/Function's Name" />
                          <TableColumn fx:id="colArchitectureType" prefWidth="75.0" text="Architecture" />
                          <TableColumn fx:id="colDegree" prefWidth="75.0" text="Degree" />
                          <TableColumn fx:id="colCycles" prefWidth="75.0" text="Result" />
                  <TableColumn prefWidth="75.0" text="Cycles" />
                      </columns>
                      <columnResizePolicy>
                          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                      </columnResizePolicy>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
                  </TableView>
            <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#onShowStatus" text="Show Status">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#onReRun" text="Re-Run">
                     <HBox.margin>
                        <Insets right="5.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
              </children>
          </VBox>
      </content>
  </TitledPane>
