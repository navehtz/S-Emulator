<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<TitledPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Available Functions" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.dashboard.components.availableFunctionsTable.AvailableFunctionsTableController">
   <content>
      <VBox alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
         <children>
            <TableView fx:id="functionsTable" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="100.0" prefHeight="199.0" prefWidth="600.0" VBox.vgrow="ALWAYS">
              <columns>
                <TableColumn fx:id="colFunctionName" minWidth="-1.0" prefWidth="30.0" text="Name" />
                <TableColumn fx:id="colProgramName" minWidth="30.0" prefWidth="75.0" text="Name of Program" />
                  <TableColumn fx:id="colUserUploaded" minWidth="30.0" prefWidth="75.0" text="User Uploaded" />
                  <TableColumn fx:id="colNumInstructions" minWidth="30.0" prefWidth="75.0" text="No. Instructions" />
                  <TableColumn fx:id="colMaxDegree" minWidth="30.0" prefWidth="75.0" text="Max Degree" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <Button fx:id="btnExecuteFunction" mnemonicParsing="false" onAction="#btnExecuteFunctionClicked" text="Execute Function">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </content>
</TitledPane>
